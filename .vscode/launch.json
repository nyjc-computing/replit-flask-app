{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Flask",
			"type": "debugpy",
			"request": "launch",
			"module": "flask",
			"env": {
				"FLASK_APP": "main.py", // Replace with your Flask app entry point
				"FLASK_ENV": "development"
